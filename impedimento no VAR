#include <iostream>
using namespace std;

int main() {
    float TA[10], TB[10], PA[10], PB[10];
    float ma = 5, mb = -5, Ma = -5, Mb = 5;
    
    for (int i = 0; i < 10; i++) {
        cin >> TA[i];
    }
    for (int i = 0; i < 10; i++) {
        cin >> TB[i];
    }
    for (int i = 0; i < 10; i++) {
        cin >> PA[i];
    }
    for (int i = 0; i < 10; i++) {
        cin >> PB[i];
    }

    for (int i = 0; i < 10; i++) {
        if (PA[i] < ma) {
            ma = PA[i];
        }
        if (PB[i] > Mb) {
            Mb = PB[i];
        }
        if (PB[i] < mb) {
            mb = PB[i];
        }
        if (PA[i] > Ma) {
            Ma = PA[i];
        }
    }

    cout << "A: ";
    for (int i = 0; i < 10; i++) {
        if (Ma < Mb) {
            cout << "sem impedimentos";
            break;
        }
        if (PA[i] > Mb) {
            cout << TA[i] << " ";
        }
    }

    cout << endl << "B: ";;
    for (int i = 0; i < 10; i++) {
        if (mb > ma) {
            cout << "sem impedimentos";
            break;
        }
        if (PB[i] < ma) {
            cout << TB[i] << " ";
        }
    }
}

